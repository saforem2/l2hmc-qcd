@article{1742-5468-2010-08-P08029,
  author={Pasquale Calabrese and Fabian H L Essler},
  title={Universal corrections to scaling for block entanglement in spin-1/2XX chains},
  journal={Journal of Statistical Mechanics: Theory and Experiment},
  volume={2010},
  number={08},
  pages={P08029},
  url={http://stacks.iop.org/1742-5468/2010/i=08/a=P08029},
  year={2010},
  abstract={We consider the R??nyi entropies ##IMG## [http://ej.iop.org/images/1742-5468/2010/08/P08029/jstat365385ieqn1.gif] {S_n(\ell)} in the one-dimensional spin-1/2 Heisenberg XX chain in a magnetic field. The case n = 1 corresponds to the von Neumann 'entanglement' entropy. Using a combination of methods based on the generalized Fisher???Hartwig conjecture and a recurrence relation connected to the Painlev?? VI differential equation we obtain the asymptotic behaviour, accurate to order ##IMG## [http://ej.iop.org/images/1742-5468/2010/08/P08029/jstat365385ieqn2.gif] {{\cal O}(\ell^{-3})} , of the R??nyi entropies ##IMG## [http://ej.iop.org/images/1742-5468/2010/08/P08029/jstat365385ieqn1.gif] {S_n(\ell)} for large block lengths ##IMG## [http://ej.iop.org/images/1742-5468/2010/08/P08029/jstat365385ieqn3.gif] {\ell} . For n = 1, 2, 3, 10 this constitutes the 3, 6, 10, 48 leading terms respectively. The o(1) contributions are found to exhibit a rich structure of oscillatory behaviour, which we analyse in some detail both for finite n and in the limit ##IMG## [http://ej.iop.org/images/1742-5468/2010/08/P08029/jstat365385ieqn4.gif] {n\to \infty} .}
}


  	
@article{unusualcardy,
  author={John Cardy and Pasquale Calabrese},
  title={Unusual corrections to scaling in entanglement entropy},
  journal={Journal of Statistical Mechanics: Theory and Experiment},
  volume={2010},
  number={04},
  pages={P04023},
  url={http://stacks.iop.org/1742-5468/2010/i=04/a=P04023},
  year={2010},
  abstract={We present a general theory of the corrections to the asymptotic behaviour of the R??nyi entropies S A ( n ) = (1 ??? n ) ??? 1 logTr?? A n which measure the entanglement of an interval A of length ##IMG## [http://ej.iop.org/images/1742-5468/2010/04/P04023/jstat352740ieqn1.gif] {\ell} with the rest of an infinite one-dimensional system, in the case when this is described by a conformal field theory of central charge c . These can be due to bulk irrelevant operators of scaling dimension x > 2, in which case the leading corrections are of the expected form ##IMG## [http://ej.iop.org/images/1742-5468/2010/04/P04023/jstat352740ieqn2.gif] {\ell^{-2(x-2)}} for values of n close to 1. However, for n > x /( x ??? 2) corrections of the form ##IMG## [http://ej.iop.org/images/1742-5468/2010/04/P04023/jstat352740ieqn3.gif] {\ell^{2-x-x/n}} and ##IMG## [http://ej.iop.org/images/1742-5468/2010/04/P04023/jstat352740ieqn4.gif] {\ell^{-2x/n}} arise and dominate the conventional terms. We also point out that the last type of corrections can also occur with x less than 2. They arise from relevant operators induced by the conical spacetime singularities necessary to describe the reduced density matrix. These agree with recent analytic and numerical results for quantum spin chains. We also compute the effect of marginally irrelevant bulk operators, which give a correction ##IMG## [http://ej.iop.org/images/1742-5468/2010/04/P04023/jstat352740ieqn5.gif] {\Or ((\log \ell)^{-2})} , with a universal amplitude. We present analogous results for the case when the interval lies at the end of a semi-infinite system.}
}

  	
