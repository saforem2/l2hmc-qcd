@article{foreman2017,
 title={RG-inspired machine learning for lattice field theory},
  volume={175},
 ISSN={2100-014X},
 DOI={10.1051/epjconf/201817511025},
 abstractNote={Machine learning has been a fast growing ﬁeld of research in
                several areas dealing with large datasets. We report recent
                attempts to use renormalization group (RG) ideas in the context
                of machine learning. We examine coarse graining procedures for
                perceptron models designed to identify the digits of the MNIST
                data. We discuss the correspondence between principal
                components analysis (PCA) and RG ﬂows across the transition for
                worm conﬁgurations of the 2D Ising model. Preliminary results
                regarding the logarithmic divergence of the leading PCA
                eigenvalue were presented at the conference. More generally, we
                discuss the relationship between PCA and observables in Monte
                Carlo simulations and the possibility of reducing the number of
                learning parameters in supervised learning based on RG inspired
                hierarchical ansatzes.},
 journal={EPJ Web of Conferences},
 author={Foreman, Sam and Giedt, Joel and Meurice, Yannick and Unmuth-Yockey,
		Judah},
 editor={Della Morte, M. and Fritzsch, P. and Gámiz Sánchez, E. and Pena Ruano,
	 C.Editors},
 year={2018},
 pages={11025} 
}

